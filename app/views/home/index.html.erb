<div class ="content">
    
<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<%=content_tag :h1, "Sign In Here:"%>
<%= form_tag("/sign_in", method: "get") do %>
    <%=label_tag(:username, "username") %>
    <%=text_field_tag(:username) %>  
    <%=label_tag(:password, "password:") %>
    <%=text_field_tag(:password) %>
    <%=submit_tag("Submit") %>
<% end %>


<%= link_to "Or Sign Up!", new_user_path %>

<%=content_tag :h1, "Current Members:"%>

<% @users.each do |u| %>
<%= link_to u.username, user_path(u) %>
<% end %>
    
</div>
